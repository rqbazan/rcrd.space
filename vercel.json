{
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/bee.js",
      "destination": "https://cdn.splitbee.io/sb.js"
    },
    {
      "source": "/_hive/:slug",
      "destination": "https://hive.splitbee.io/:slug"
    }
  ]
}
